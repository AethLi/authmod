  "devDependencies": {
    "husky": "^1.3.1",
    "lint-staged": "^8.1.3",
    "prettier": "^1.16.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all",
    "printWidth": 80
  },
  "lint-staged": {
    "*.j": [
      "./gradlew spotlessApply --stacktrace --info --scan",
      "git add"
    ],
    "*.md": [
      "prettier --write",
      "git add"
    ]
  }
}
